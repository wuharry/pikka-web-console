{
  "name": "pikka-web-console",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:frontend\" \"pnpm dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --exec tsx src/backend/main.ts",
    "dev:backend-watch": "tsx watch src/backend/main.ts",
    "build": "tsc && vite build",
    "build:frontend": "vite build",
    "build:backend": "tsc",
    "preview": "vite preview",
    "start:backend": "tsx src/backend/main.ts"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^24.3.0",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.4",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  },
  "peerDependencies": {
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3"
  },
  "packageManager": "pnpm@9.0.0+sha512.b4106707c7225b1748b61595953ccbebff97b54ad05d002aa3635f633b9c53cd666f7ce9b8bc44704f1fa048b9a49b55371ab2d9e9d667d1efe2ef1514bcd513",
  "dependencies": {
    "@hono/node-server": "^1.18.2",
    "hono": "^4.9.2"
  }
}
